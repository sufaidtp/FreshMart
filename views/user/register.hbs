	<script>
		setTimeout(() => {
			const val = document.getElementById('inemail')
			val.style.display = "none"
			const rege = document.getElementById('rege')
			rege.style.display = "none"
		}, 5000)

		const nameError = document.getElementById('usernameErr')
		const phoneError = document.getElementById('phoneErr')
		const emailError = document.getElementById('emailErr')
		const passError = document.getElementById('passwordErr')
		const conpassError = document.getElementById('conpasswordErr')


		function validateUsername() {
			name = document.getElementById('username').value
			if (name.length == 0) {
				nameError.innerHTML = 'Enter the username'
				nameError.style.color = 'red'
				return false
			}
			if (!name.match(/^[a-zA-Z0-9_]{3,20}/)) {
				nameError.innerHTML = 'minimun 3 characters'
				nameError.style.color = 'red'
				return false
			}
			nameError.innerHTML = ''
			return true
		}
		function validatephone() {
			const phone = document.getElementById('phone').value
			if (phone.length == 0) {
				phoneError.innerHTML = 'Enter the phoneNumber'
				phoneError.style.color = 'red'
				return false
			}
			if (!phone.match(/^\d{10}/)) {
				phoneError.innerHTML = 'Enter a vaild number'
				phoneError.style.color = 'red'
				return false
			}
			if (phone.length > 10) {
				phoneError.innerHTML = 'Length of the number is more than 10'
				phoneError.style.color = 'red'
				return false
			}
			phoneError.innerHTML = ''
			return true

		}
		function validateemail() {
			const email = document.getElementById('email').value
			if (email.length == 0) {
				emailError.innerHTML = 'Enter the emailAddress'
				emailError.style.color = 'red'
				return false
			}
			if (!email.match(/^[A-Za-z\._\-[0-9]*[@][A-Za-z]*[\.][a-z]{2,4}$/)) {
				emailError.innerHTML = 'Enter a vaild emailaddress'
				emailError.style.color = 'red'
				return false
			}
			emailError.innerHTML = ''
			return true

		}
		function validatepassword() {
			const pass = document.getElementById('pass').value
			if (pass.length == 0) {
				passError.innerHTML = 'Enter the password'
				passError.style.color = 'red'
				return false
			}
			if (!pass.match(/^[A-Za-z0-9]{8,}/)) {
				passError.innerHTML = 'minimum 8 characters'
				passError.style.color = 'red'
				return false
			}
			passError.innerHTML = ''
			return true

		}
		function validateconpassword() {
			const pass = document.getElementById('pass').value
			const conpass = document.getElementById('conpass').value
			if (pass.length == 0) {
				conpassError.innerHTML = 'Enter the password first'
				conpassError.style.color = 'red'
				return false
			}
			if (pass != conpass) {
				conpassError.innerHTML = 'password not match, enter correct password'
				conpassError.style.color = 'red'
				return false
			}
			conpassError.innerHTML = ''
			return true
		}

		function validateForm() {
			if (!validateUsername() || !validateemail() || !validatepassword() || !validatephone() || !validateconpassword()) {

				return false
			}
			return true;
		}
	</script>


