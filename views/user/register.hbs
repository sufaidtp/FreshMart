						<div class="block-title">
							<h2 class="title"><span>Create Account</span></h2>
						</div>

						<form method="post">

							<p id="rege" style="color: red;">{{uname}}</p>

							<div class="form-group">

								<label>User Name</label>
								<input type="text" value="{{username}}" name="username" onkeyup="validateUsername()"
									id="username">
								<span id="usernameErr"></span>
							</div>

							<div class="form-group">
								<label>Email</label>
								<input type="text" value="{{email}}" name="email" onkeyup="validateemail()" id="email">
								<span id="emailErr"></span>
							</div>

							<div class="form-group">
								<label>mobile No</label>
								<input type="number" value="{{mobileno}}" name="phone" onkeyup="validatephone()"
									id="phone">
								<span id="phoneErr"></span>
							</div>

							<div class="form-group">
								<label>Password</label>
								<input type="password" value="{{password}}" name="password" onkeyup="validatepassword()"
									id="pass">
								<span id="passwordErr"></span>
							</div>

							<div class="form-group">
								<label>confirm Password</label>
								<input type="password" value="" name="confirm_password" onkeyup="validateconpassword()"
									id="conpass">
								<span id="conpasswordErr"></span>
							</div>

							<div class="form-group text-center">
								<button type="submit" class="btn btn-primary" onclick="return validateForm()">
									Create Account
								</button>
							</div>
						</form>
					</div>
				</div>
			</div>
